	  <PackageReference Include="Microsoft.ML.OnnxRuntime" Version="1.16.3" />
	  <PackageReference Include="Microsoft.ML.OnnxTransformer" Version="3.0.0" />
	  <PackageReference Include="Syncfusion.Blazor" Version="23.2.6" />
