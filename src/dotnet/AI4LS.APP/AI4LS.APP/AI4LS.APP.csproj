	  <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
	</ItemGroup>
	<ItemGroup>
	  <Content Update="CountryCodes.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Update="LUCAS-SOIL-all-2018.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="wwwroot\images\team\" />
	</ItemGroup>
	<ItemGroup>
	  <None Update="hacker.png">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
