@using Microsoft.AspNetCore.Components.Web
@namespace AI4LS.APP.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@using Syncfusion.Blazor

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no" />


    <base href="~/" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="css/team.css" rel="stylesheet" />
    <link href="css/chat.css" rel="stylesheet" />
    <link href="css/classifier.css" rel="stylesheet" />
    <link href="css/predict.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel='nofollow' href="_content/Syncfusion.Blazor/styles/material.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://js.arcgis.com/4.28/esri/themes/light/main.css">



    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />


</head>

<body>
    @RenderBody()

    <script autostart="false" src="_framework/blazor.server.js"></script>
    <script src="https://kit.fontawesome.com/5c8597d474.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="_content/Syncfusion.Blazor/scripts/syncfusion-blazor.min.js" type="text/javascript"></script>
    <script src="main.js"></script>
    <script src="d3.v7.min.js"></script>
    <script src="https://js.arcgis.com/4.28/"></script>

    <script>
        Blazor.start();
    </script>

</body>
</html>
